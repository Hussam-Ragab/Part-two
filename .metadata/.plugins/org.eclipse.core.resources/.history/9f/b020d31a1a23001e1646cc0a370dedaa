
/*
 *  Write a C Function that takes one character and checks if it alphabet or not.
 *
 * */

#include <stdio.h>

#define FALSE 0
#define TRUE  1

void Check_Alphabet(char Charachter);


int main(void)
{
	char ch;
	printf("enter character\n");
	fflush(stdout);
	scanf("%c",&ch);
	Check_Alphabet(ch);

	return 0;
}


void Check_Alphabet(char Charachter)
{
	if((Charachter >= 'A' && Charachter <= 'Z') || (Charachter >= 'a' && Charachter <= 'z'))
		printf("Charachter %c is Alphabet\n",Charachter);
	else
		printf("Charachter %c is not Alphabet\n",Charachter);

}
